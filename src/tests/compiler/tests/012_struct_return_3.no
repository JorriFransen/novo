
Vec :: struct {
    x: s64;
    y: s64;
}

vec :: (x: s64, y: s64) -> Vec {
    result: Vec;
    result.x = x;
    result.y = y;
    return result;
}

vec_add :: (a: Vec, b: Vec) -> Vec {
    result: Vec;
    result.x = a.x + b.x;
    result.y = a.y + b.y;
    return result;
}

main :: () -> s64 {

    p1 := vec(11, 22);
    p2 := vec(22, 11);

    r := vec_add(p1, p2);

    return r.x + r.y;
}

