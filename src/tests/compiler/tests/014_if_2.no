
t1 :: () -> s64 {
    i := 0;

    if (i == 0) {
        return 1;
    } else {
        return 2;
    }
}

t2 :: () -> s64 {
    i := 1;

    if (i == 0) {
        return 3;
    } else {
        return 4;
    }
}

t3 :: () -> s64 {
    i := 0;
    result : s64;

    if (i == 0) {
        result = 5;
    } else {
        result = 6;
    }

    return result;
}

t4 :: () -> s64 {
    i := 1;
    result : s64;

    if (i == 0) {
        result = 7;
    } else {
        result = 8;
    }

    return result;
}

main :: () -> s64 {
    return t1() + t2() + t3() + t4();
}

