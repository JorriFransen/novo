Vec :: struct {
    x: s64;
    y: s64;
    z: s64;
}

main :: () -> s64 {
    // return 72623859790382856; // 0x102030405060708
    // return 1 + 2;
    // return add(11, 22);
    // return add(11, 22) + add(1, 2);

    p : Vec;
    p.x = 11;
    p.y = 02;
    p.z = 20;

    p2 := p;
    p2 = p;

    p2.x = p2.z / 2;
    p2.y = p2.y + 1;

    return add(add(p.x, p.y), p.z) + add(add(p2.x, p2.y), p2.z);
}

add :: (a: s64, b: s64) -> s64 {
    result : s64;
    result = a + b;
    return result;
    // return a + b;
}
